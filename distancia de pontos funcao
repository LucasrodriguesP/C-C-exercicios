#include <stdio.h>
#include <math.h>
float DistanciaPontos(float x2,float x1,float y2,float y1);
int main(){
    double x1,x2,y1,y2;
    printf("x1 = ");
    scanf("%lf",&x1);
    printf("x2 = ");
    scanf("%lf",&x2);
    printf("y1 = ");
    scanf("%lf",&y1);
    printf("y2 = ");
    scanf("%lf",&y2);
    DistanciaPontos(x2,x1,y2,y1);
    return 0;
}
float DistanciaPontos(float x2,float x1,float y2,float y1){
    float resultado;
    resultado = sqrt(pow((x2 - x1),2) + pow((y2 - y1),2));
    return printf("%f", resultado);
}
